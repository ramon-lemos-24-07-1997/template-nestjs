# .env
# ===========================================
# CONFIGURAÇÕES DA APLICAÇÃO
# ===========================================

# Ambiente da aplicação
NODE_ENV=development
PORT=4000

# ===========================================
# CONFIGURAÇÕES DE BANCO DE DADOS
# ===========================================

# Para serviços locais (descomente se tiver PostgreSQL local)
DATABASE_URL=postgresql://postgres:docker_password@localhost:5432/temp_db

# Para serviços Docker (comente a linha acima e descomente a abaixo)
# DATABASE_URL=postgresql://postgres:docker_password@localhost:5432/temp_db

# ===========================================
# CONFIGURAÇÕES RABBITMQ
# ===========================================

# Para serviços locais (descomente se tiver RabbitMQ local)
RABBITMQ_URL=amqp://localhost

# Para serviços Docker (comente a linha acima e descomente a abaixo)
# RABBITMQ_URL=amqp://docker_user:docker_pass@localhost:5672

# ===========================================
# CONFIGURAÇÕES REDIS
# ===========================================

# Para serviços locais (descomente se tiver Redis local)
REDIS_URL=redis://localhost:6379

# Para serviços Docker (comente a linha acima e descomente a abaixo)
# REDIS_URL=redis://localhost:6379

# ===========================================
# CONFIGURAÇÕES DE LOGS
# ===========================================
LOG_LEVEL=info


# ===========================================
# CONFIGURAÇÕES DE SEGURANÇA
# ===========================================

# Chave secreta para JWT
JWT_SECRET=your-super-secret-jwt-key-here


